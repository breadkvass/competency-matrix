{
  "name": "competency-matrix",
  "scripts": {
    "build:web": "npx expo export --platform web -c --output-dir dist",
    "dev": "npm run start",
    "serve:app": "expo start --dev-client",
    "serve:web": "npx serve dist",
    "setup": "npm install expo && expo install",
    "start": "expo start --go",
    "upgrade": "expo upgrade",
    "web": "expo start --web --no-dev --host localhost --port 3000"
  },
  "overrides": {
    "cookie": "^0.7.0"
  },
  "dependencies": {
    "@fanchenbao/react-native-scroll-indicator": "^0.4.0",
    "@react-navigation/native": "^7.0.0",
    "expo": "~52.0.7",
    "expo-app-loading": "^2.1.1",
    "expo-dev-client": "~5.0.1",
    "expo-font": "~13.0.4",
    "expo-linking": "~7.0.2",
    "expo-router": "~4.0.5",
    "expo-splash-screen": "^0.29.22",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-animatable": "^1.4.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-select-dropdown": "^4.0.1",
    "react-native-svg": "^15.11.2",
    "react-native-web": "~0.19.13",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@types/react": "~18.3.12",
    "@types/styled-components": "^5.1.34",
    "@types/styled-components-react-native": "^5.2.5",
    "@types/uuid": "^10.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "globals": "^15.12.0",
    "typescript": "^5.6.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
